import java.util.Scanner;
public class Main {

    static boolean isEven(int a) {
        if (a % 2 == 0) {
            return true;
        } else {
            return false;
        }
    }

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int b = in.nextInt();
        for (int i = 1; i <= b; i++) {
            if (isEven(i)) {
                System.out.println(i);
            }
        }
    }
}
